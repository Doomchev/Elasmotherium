структура Строка<ЛюбойСимвол ТипСимвола, ЛюбоеЦелое ТипИндекса> расширяет Массив<ТипСимвола> {
  создать(ЛюбоеЧисло число);
  создать(Объект объект) -> объект.вСтроку;
	
  ТакойЖе нижнийРегистр() {
		ТакаяЖе новаяСтрока = создатьТакуюЖе(размер);
		для(Целого и от 0 до размера) новаяСтрока[и] = эта[и].нижнийРегистр;
		вернуть новуюСтроку;
	}
	
  ТакойЖе верхнийРегистр() {
		ТакойЖе новаяСтрока = создатьТакуюЖе(размер);
		для(Целого и от 0 до размер) новаяСтрока[и] = эта[и].верхнийРегистр;
		вернуть новуюСтроку;
	}
	
	ТакойЖе обрезать() {
		ТипаИндекса от = 0, до = размер - 1;
		пока(от < размер && эта[от] <= " ") от++;
		если(от равен размеру) вернуть "";
		пока(эта[до] <= " ") до++;
		вернуть эту[от ..= до];
	}

	Массив<ТакойЖе> разбить(ТипСимвола символ);
	
	Вопрос начинаетсяС(Строкового начала) {
		если(начала.размер больше размера) вернуть нет;
		для(Целого и от 0 до начала.размера) если(эта[и] != началу[и]) вернуть нет;
		вернуть да;
	}
}