числ[о] = Целое.случайное(, до 100)
повторять
	предположени[е] = Целое.спросить("Какое число я загадал?")
	если(предположение меньше числа)
		сообщить("Ваше число слишком маленькое!")
	иначе если(предположение больше числа)
		сообщить("Ваше число слишком большое!")
	иначе
		сообщить("Вы правы!")
		выйти()
}