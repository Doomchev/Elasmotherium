native print(String value)

native List<ValueType> {
	ValueType get first
	ValueType get last
	ValueType atIndex(Integer index)
	addFirst(ValueType value)
	addLast(ValueType value)
}

Player {
	($firstName, $lastName, $age)
	String firstName, lastName
	age = 18
	incrementAge() -> $age++
	get description -> "\($firstName) \($lastName), \($age) years old"
}

description = " is looking around"
player = Player("John", "Smith", 36)
player.incrementAge()
print(player.description + description)

